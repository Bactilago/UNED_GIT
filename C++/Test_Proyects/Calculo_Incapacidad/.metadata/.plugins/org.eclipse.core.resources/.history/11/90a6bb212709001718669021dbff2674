int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	Fechas hoy;
	hoy.setNow();
	hoy.setTime();
	hoy.getCurrYear();
	hoy.setFuture();

	return 0;
}
