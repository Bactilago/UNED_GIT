#include "Setter.h"

Setter::Setter() {
	name = "George";
	hoy = time(0);


}

string Setter::toString(){
	return "Person's name is: " + name;
}

void Setter::setName(string newName) {
	name = newName;
}


tm Setter::getTime(time_t &hoy){
	tm *ltm = localtime(&hoy);
	return *ltm;
}
